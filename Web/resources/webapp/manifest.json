{
	"_version": "1.5.0",
	"sap.app": {
		"id": "osr.edw.data.info.Web",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "hanatemplates.basicSAPUI5ApplicationProject",
			"version": "0.0.0"
		},
		"dataSources": {
			"dropdownJSON": {
				"uri": "model/dropdownModel.json",
				"type": "JSON",
				"setting": {}
			},
			"metadataRemote": {
				"uri": "/org/srv/xs/sourceMetadata.xsodata",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "osr.edw.data.info.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "osr.edw.data.info.i18n.i18n"
				}
			},
			"dropdownModel": {
				"dataSource": "dropdownJSON"
			},
			"metadataModel": {
				"dataSource": "metadataRemote"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}],
			"js": [{
				"uri": "asset/lib/moment-with-locales.js"
			}, {
				"uri": "asset/lib/moment-business-days.js"
			}, {
				"uri": "asset/lib/bluebird.min.js"
			}]
		},
		"config": {
			"searchHelper": "/osr/edw/source/data/search/"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "osr.edw.data.info.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [{
				"pattern": "",
				"name": "homepage",
				"target": "detailobject"
			}, {
				"pattern": "home",
				"name": "home",
				"target": "detailobject"
			}, {
				"pattern": "detailobject",
				"name": "detailobject",
				"target": "detailobject"
			}],
			"targets": {
				"home": {
					"viewType": "XML",
					"viewName": "DetailObject",
					"transition": "slide",
					"clearAggregation": true
				},
				"detailobject": {
					"viewType": "XML",
					"viewName": "DetailObject",
					"transition": "slide"
				},
				"notFound": {
					"viewType": "XML",
					"viewName": "NotFound",
					"controlAggregation": "pages"
				}
			}
		}
	}
}