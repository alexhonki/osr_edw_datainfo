<core:FragmentDefinition xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
                         xmlns:core="sap.ui.core"
                         xmlns:f="sap.ui.layout.form"
                         xmlns:l="sap.ui.layout"
                         xmlns:mvc="sap.ui.core.mvc"
                         xmlns="sap.m">

  <OverflowToolbar>
    <ToolbarSpacer />
    <Button type="Emphasized"
            press="onCancelPressed"
            text="Cancel" />
    <Button type="Emphasized"
            enabled="{= ${footerModel>/poUpdateEnable} === true }"
            press="onUpdatePressed"
            text="Update" />
    <Button type="Emphasized"
            press="onClearAllPressed"
            text="Clear All" />

  </OverflowToolbar>


</core:FragmentDefinition>
