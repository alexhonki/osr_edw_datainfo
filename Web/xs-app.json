{
	"authenticationMethod": "route",
	"routes": [{
		"source": "/osr/edw/source/data/metadata/(.*)",
		"destination": "Services_url",
		"csrfProtection": true,
		"scope": "$XSAPPNAME.DATA_INFO",
		"authenticationType": "xsuaa"
	},{
		"source": "/api/static(.*)",
		"destination": "Services_url",
		"csrfProtection": true,
		"authenticationType": "xsuaa"
	}, {
		"source": "/metadata/srv/xs(.*)",
		"destination": "Services_url",
		"csrfProtection": true,
		"authenticationType": "xsuaa"
	}, {
		"source": "^/(.*)$",
		"localDir": "resources",
		"scope": ["$XSAPPNAME.DATA_INFO"],
		"authenticationType": "xsuaa",
		"replace": {
			"pathSuffixes": ["index.html"],
			"vars": ["ui5liburl"]
		}
	}]
}
